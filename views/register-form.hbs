<!DOCTYPE html>
<html>

<head>
  <meta charset="utf-8">
  <title></title>
</head>

<body>
  <section class="register-container">
    <form class="register" role="register-form">
      <label for="register__name">Restaurant</label>
      <input type="text" class="name">
      <label for="register__address">Address</label>
      <input type="text" placeholder="i.e. 123 Narkal Street Glitter, CO 12345" class="register__address">
      <label for="register__phone">Phone</label>
      <input type="text" placeholder="###-###-####" class="register__phone">
      <label for="register__hours">Hours</label>
      <input type="text" placeholder="i.e. 10am-10pm" class="register__hours">
      <label for="register__email">Email</label>
      <input type="email" placeholder="restaurant@gmail.com" class="register__email">
      <label for="register__menu">Menu</label>
    </form>
    <form>
      <p>Expense Level</p>
      <input type="checkbox" class="register__expense">
      <label for="register__expense">$</label>
      <input type="checkbox" class="register__expense">
      <label for="register__expense">$$</label>
      <input type="checkbox" class="register__expense">
      <label for="register__expense">$$$</label>
      <input type="checkbox" class="register__expense">
      <label for="register__expense">$$$$</label>
    </form>
    <!-- add action="file path" -->
    <form>
      <input type="file" class="register__menu">
      <input type="submit" class="add-menu" accept="image/*">
    </form>
    <!-- add social media not sure how to add accounts -->
  </section>

</body>

</html>
